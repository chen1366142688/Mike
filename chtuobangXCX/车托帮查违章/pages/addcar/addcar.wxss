page{
    font-family: "PingFang SC";
    background: #efefef;
    position: relative;
    height: 1206rpx;
    overflow: hidden;
}
.fl {
    float: left;
}
.fr {
    float: right;
}
/*清浮动*/
.clearfix:after {
    display: block;
    content: '';
    clear: both;
}
.clearfix {
    zoom: 1;
}
#alert_info{
    width: 750rpx;
    height: 1206rpx;
    text-align: center;
    position: absolute;
    top: 0;
    left: 0;
    z-index: 999;
    background: rgba(0,0,0,0.5);
    padding-top: 309rpx;
    display: none;
}
#alert .license{
    width: 663rpx;
    height: 482rpx;
    margin-left: 44rpx;
}
.content{
    height: 100%;
    padding: 0 26rpx;
    box-sizing: border-box;
    -webkit-box-sizing: border-box;
}
.car {
    height: 90rpx;
    line-height: 90rpx;
    font-size: 28rpx;
}
.phone_personal{
  color:#434343;
  padding: 26rpx 0;
  font-size: 32rpx;
}
.phone_one{
  display: flex;
  position: relative;
  justify-content: space-between;
  background-color:#fff;
  width:136rpx;
  border-top-left-radius: 10rpx;
  border-bottom-left-radius: 10rpx;
  font-size: 32rpx;
  padding-left: 38rpx;
}
.person_box{
  position: relative;
}
.area_select{
    margin-top:0;
    z-index:100;
    position:absolute;
    background:#d6d8dd;
    padding-top:30rpx;
    left: 0;
    padding-left: 30rpx;
    bottom: 0;
    margin-right: -25rpx;
    width: 720rpx;
}
.select_one{
    text-align:center;
    background-color:#fdfdfe;
    float:left;
    width:74rpx;
    height:74rpx;
    margin-bottom:20rpx;
    border:1px solid #ccc;
    line-height:76rpx;
    border-radius:8rpx;
    font-size:34rpx;
}
.personal_image{
  z-index: 100;
  position: absolute;
  right:2.5%;
  width: 13rpx;
  height: 23rpx;
  margin:40rpx 20rpx 40rpx 0;
  transition: All 0.4s ease; 
  -webkit-transition: All 0.4s ease;

}
.rotateRight{
  transform: rotate(90deg);
}
#chePai{
    display: inline-block;
    font-size: 32rpx;
    width: 490rpx;
    padding: 25rpx 0;
    border: none;
    border-top-right-radius: 12rpx;
    border-bottom-right-radius: 12rpx;
    background: #FFF;
    margin-left:2rpx;
    padding-left:30rpx;
}
.db2{
    margin-top: 115rpx;
}
.chejia, .fadong {
    width: 670rpx;
    padding: 28rpx 0;
    background-color: #fff;
    border-radius: 12rpx;
    margin: 0 auto;
    margin-bottom: 15rpx;
    border: none;
    font-size: 32rpx;
    padding-left:30rpx;
}
.check{
    width: 2rpx;
    height: 2rpx;
}
label{
    margin-left: 30rpx;
}
.span{
    font-size: 30rpx;
    color: #535353;
}
#submit {
    display: block;
    width: 699rpx;
    border-radius: 12rpx;
    text-align: center;
    color: #fff;
    border: 0;
    font-size: 36rpx;
    height: 110rpx;
    line-height: 110rpx;
    margin-top: 15rpx;
    background-color: #3b7eec;
}